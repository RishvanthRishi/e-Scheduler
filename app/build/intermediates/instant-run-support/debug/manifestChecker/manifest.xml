<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.girish.e_scheduler"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="23" />

    <application
        android:allowBackup="true"
        android:debuggable="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:testOnly="true"
        android:theme="@style/AppTheme" >
        <activity android:name="com.example.girish.e_scheduler.login" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.girish.e_scheduler.AuthoirtyActivity"
            android:label="e-Scheduler" />
        <activity android:name="com.example.girish.e_scheduler.Pankaj" />
        <activity android:name="com.example.girish.e_scheduler.Tapaswi" />
        <activity android:name="com.example.girish.e_scheduler.Anupam" />
        <activity android:name="com.example.girish.e_scheduler.MainActivity" />
        <activity android:name="com.example.girish.e_scheduler.Rajendra" />
        <activity android:name="com.example.girish.e_scheduler.Wilfred" >
        </activity>

        <provider
            android:name="com.android.tools.ir.server.InstantRunContentProvider"
            android:authorities="com.example.girish.e_scheduler.com.android.tools.ir.server.InstantRunContentProvider"
            android:multiprocess="true" />
    </application>

</manifest>